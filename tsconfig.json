{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es2017",
    "allowJs": true,
    "checkJs": true,
    "outDir": "./bin",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "resolveJsonModule": true
  },
  "include": [
      "src/**/*.ts" , 
      // "src/workers/aws/data/*.json"
  ],
  "exclude": [
    "node_modules"
  ]
}